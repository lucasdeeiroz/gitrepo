(function(){"use strict";var n=angular.module("devmasterApplication",["directives","services"]);n.controller("homeController",["$scope","PostModel",function(n,t){function i(){n.getLatest()}n.getPublished=function(){t.getPublished({},function(t){n.postList=t.Resource;n.TotalItens=t.TotalItens;n.loaded=!0})};n.getLatest=function(){t.getLatest({},function(t){n.lastPost=t.Resource})};i()}])})()